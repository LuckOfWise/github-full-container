{
	"manifest_version": 2,
	"author": "LuckOfWise",
	"name": ".world Demo Manager",
	"description": "ソニックガーデンの.worldのデモ用のツールです",
	"version": "1.0",
	"icons": {
		"48": "icon.png",
		"128": "sg_icon_128.png"
	},
	"content_scripts": [{
		"matches": ["https://www.sonicgarden.world//*"],
		"js": ["jquery-3.4.1.min.js", "script.js"]
	}],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "",
		"default_popup": "popup.html"
	},
	"permissions": [
		"background"
	]
}
