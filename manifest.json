{
	"manifest_version": 2,
	"author": "LuckOfWise",
	"name": "Github full container",
	"description": "githubのページのcontainerのwidthを広くするためのChromeエクステンション",
	"version": "1.0",
	"icons": {
		"48": "icon.png"
	},
	"content_scripts": [{
		"matches": ["https://github.com/*"],
		"js": ["script.js"]
	}],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": ""
	},
	"permissions": [
		"tabs",
		"background",
		"http://*/*",
		"https://*/*"
	]
}